{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Name }} ",
          "foreground": "#ffffff",
          "type": "shell",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#ffe9aa",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_host": true
          },
          "template": " {{ if .SSHSession }}\uf817{{ end }}{{ .UserName }} ",
          "foreground": "#100e23",
          "background": "#ffffff",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\ueb06",
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "foreground": "#1e1e2e",
          "background": "#89b4fa",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#193549",
          "background": "#95ffa4",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#f17c37{{ end }}",
            "{{ if gt .Behind 0 }}#89d1dc{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue20f{{ if gt .Code 0 }}\uf00d {{ .Meaning }} {{ .Code }} {{ else }}\uf42e{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#ff8080",
          "type": "status",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "nix-shell",
          "template": "(nix-{{ .Type }}) ",
          "foreground": "blue",
          "background": "transparent",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uebd8  {{ .Name }} / {{ .User.Name }} ",
          "foreground": "#1e1e2e",
          "background": "#89b4fa",
          "type": "az",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "home_enabled": true
          },
          "template": " \ue235  {{ .Venv }} - {{ .Full }} ",
          "foreground": "#1e1e2e",
          "powerline_symbol": "\ue0b0",
          "background": "#cba6f7",
          "type": "python",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#89b4fa",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
